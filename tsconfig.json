{
  "compilerOptions": {
    "baseUrl": ".",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": [
        "es5",
        "es6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "paths": {
      "@src/*": [ "./src/*" ]
    },
    "sourceMap": true,
    "target": "es5",
  }
}
