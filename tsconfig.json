{
  "compilerOptions": {
    "baseUrl": ".",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": [
        "es5",
        "es6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "paths": {
      "@src/*": [ "./src/*" ]
    },
    "target": "es5"
  }
}
